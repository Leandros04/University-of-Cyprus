public class DNASequencer {
	
	public static void main(String[] args)
	{
		String BASE_STRAND = args[0];
		
		boolean invalidInput = false;
		
		for(int i = 0; i < BASE_STRAND.length(); i++)
		{
			if(BASE_STRAND.charAt(i) != 'a' || BASE_STRAND.charAt(i) != 'c' || BASE_STRAND.charAt(i) != 'g' || BASE_STRAND.charAt(i) != 't' || BASE_STRAND.length() == 0)
			{
				invalidInput = true;
			}
		}
		
		String STRAND = args[1];
				
		for(int i = 0; i < STRAND.length(); i++)
		{
			if(STRAND.charAt(i) != 'a' || STRAND.charAt(i) != 'c' || STRAND.charAt(i) != 'g' || STRAND.charAt(i) != 't' || STRAND.length() == 0)
			{
				invalidInput = true;
			}
		}
		
		if(invalidInput == true)
		{
			System.out.print("Wrong Input.");
		}
		
		int MIN_COVER = 4; 
		int COVER_COUNT = 0; 
		int MAX_COVER = 0; 
		String COVER = ""; 
		
		for(int i = BASE_STRAND.length() -1; i >= 0; i--) 
		{
			for(int j = 0; j < STRAND.length(); j++)
			{
				while(j < BASE_STRAND.length())
				{
					if(BASE_STRAND.charAt(BASE_STRAND.length() -1) == STRAND.charAt(j))
					{
						COVER_COUNT++;
						COVER += STRAND.charAt(j);				
					}
				
					if(COVER_COUNT > MAX_COVER)
					{
						MAX_COVER = COVER_COUNT;
					}
				}
			}
		}
	}
}